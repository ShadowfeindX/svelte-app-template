{
  "compilerOptions": {
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "outDir": "../public",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "forceConsistentCasingInFileNames": true,
    "lib": ["es6", "es2017", "webworker", "ESNext"]
  },

  "compileOnSave": true,
  "include": [
    "src"
  ]
}
